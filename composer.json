{
    "name": "tkivelip/laravel-sql-executer",
    "description": "Laravel SQL Executer",
    "license": "MIT",
    "type": "package",
    "authors": [
        {
            "name": "Tobi Kivelip",
            "email": "tkivelip@gmail.com",
            "homepage": "https://github.com/tkivelip",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1.3",
        "laravel/framework": "5.7.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "tkivelip\\LaravelSqlExecuter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tkivelip\\LaravelSqlExecuter\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "tkivelip\\LaravelSqlExecuter\\Providers\\SqlExecuterServiceProvider"
            ]
        }
    }
}
